{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "urlPattern": "/metadata/identity/oauth2/token\\?.*",
        "headers": {
          "Metadata": {
            "equalTo": "True"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{\"access_token\":\"test-token\",\"token_type\":\"Bearer\",\"expires_in\":3600}"
      }
    }
  ]
}
