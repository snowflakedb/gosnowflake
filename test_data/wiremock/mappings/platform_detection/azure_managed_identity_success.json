{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "urlPattern": "/metadata/identity/oauth2/token\\?.*",
        "headers": {
          "Metadata": {
            "equalTo": "true"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "jsonBody": {
          "access_token": "test-token",
          "token_type": "Bearer",
          "expires_in": 3600
        }
      }
    }
  ]
}
