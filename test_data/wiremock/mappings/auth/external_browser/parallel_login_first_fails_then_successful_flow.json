    {
      "scenarioName": "External browser parallel login first fails then successful flow",
      "requiredScenarioState": "Started",
      "newScenarioState": "First request failed",
      "request": {
        "urlPathPattern": "/session/authenticator-request.*",
        "method": "POST",
        "bodyPatterns": [
          {
            "equalToJson" : {
              "data": {
                "LOGIN_NAME": "testUser"
              }
            },
            "ignoreExtraElements" : true
          },
          {
            "matchesJsonPath": {
              "expression": "$.data.TOKEN",
              "absent": "(absent)"
            }
          }
        ]
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "code": null,
          "message": "auth failed",
          "success": false
        },
        "fixedDelayMilliseconds": 2000
      }
    },
