{
  "mappings": [
    {
      "request": {
        "urlPathPattern": "/session/v1/login-request.*",
        "method": "POST",
        "bodyPatterns": [
          {
            "equalToJson" : {
              "data": {
                "PASSWORD": "INVALID_PASSWORD"
              }
            },
            "ignoreExtraElements" : true
          }
        ]
      },
      "response": {
        "status": 200,
        "jsonBody": {
          "data": null,
          "code": "390100",
          "message": "Incorrect username or password was specified.",
          "success": false
        }
      }
    }
  ]
}
